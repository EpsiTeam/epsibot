{
	"name": "epsibot",
	"version": "2.0.0",
	"description": "Light discord bot for epsilon needs",
	"main": "built/index.js",
	"scripts": {
		"clean": "rm -rf built/ logs/",
		"check": "npx eslint src/",
		"fix": "npx eslint --fix src/",
		"build": "rm -rf built/ && npm run check && node_modules/typescript/bin/tsc",
		"migrate": "node node_modules/typeorm/cli.js migration:run",
		"start": "npm run build && node -r dotenv/config built/main.js",
		"start:builded": "node -r dotenv/config built/main.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"private": true,
	"author": "EpsiTeam",
	"license": "ISC",
	"type": "module",
	"dependencies": {
		"@discordjs/builders": "^0.12.0",
		"@discordjs/rest": "^0.3.0",
		"@types/node": "^17.0.21",
		"better-sqlite3": "^7.5.0",
		"discord-api-types": "^0.29.0",
		"discord.js": "^13.6.0",
		"dotenv": "^16.0.0",
		"moment": "^2.29.1",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.2.45",
		"typescript": "^4.6.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"eslint": "^8.11.0"
	}
}
